<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eomsbd.kon_dowa_porle_ki_hoy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kon_dowa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kon_dowa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.eoms.kon_dowa_porle_ki_hoy.SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.eoms.kon_dowa_porle_ki_hoy.StartActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name="com.eoms.kon_dowa_porle_ki_hoy.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>